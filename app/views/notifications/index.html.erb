<p style="color: green"><%= notice %></p>

<h1>Your Notifications!</h1>

<div id="notifications">
  <% @notifications.each do |notification| %>
    <p>
      <%= link_to "#{notification.to_notification.message}: #{notification.talk_title} at #{notification.talk_start_time}", talk_path(notification.talk)%>
    </p>
  <% end %>
</div>

